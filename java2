package probleme; 
import java.util.*;
public class java2 {

private static Scanner input;
public static void main(String[] args) {

int n,m,i,j,k;

System.out.print ("Introduceti numarul de elemente ale multimii A (n) = ");
input = new Scanner(System.in);
n = input.nextInt ();

System.out.print ("Introduceti numarul de elemente ale multimii B (m) = ");
m = input.nextInt (); 
int A[]= new int[n]; 
int B[]= new int[m];
System.out.println("Introduceti elementele multimii A: ");
for(i=0;i<n;i++){
System.out.print("A["+i+"]=");
A[i]=input.nextInt ();
}
System.out.println("Introduceti elementele multimii B: "); 
for(i=0; i<m; i++) {
System.out.print("B["+i+"]=");
B[i]=input.nextInt ();
}
int C[]= new int [100];
// copiem elementele dn multimea A in multimea c
for(i=0;i<n;i++) C[i]=A[i];
k=n; // k ceprezinta numarul de elemente din multimea C for(j=0; j<n; j++){
for(j=0;j<m;j++) {
boolean ok=true;
for(i=0;i<n;i++) 
if(B[j] ==A[i]) ok=false;
if(ok==true){
C[k]=B[j];
k++;
}
}
for(i=0; i<k; i++)
System.out.print(C[i]+" ");
}
}
